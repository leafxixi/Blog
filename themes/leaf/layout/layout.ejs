<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="chrome=1">


    <title>
      <%= page.title || '' %> 
    </title>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <% if (config.author){ %>
      <meta name="author" content="<%= config.author %>">
    <% } %>
    <% if ( is_home() && config.description){ %>
      <meta name="description" content="<%= config.description %>">
    <% } %>

  
    <%- css('css/leaf.css') %>

</head>
<body>

    <header class="headline">
        <div style="float: left">BUTTON</div>
        <div style="float: right"><h2>logo</h2></div>
    </header>
    <div class="main-panel">
      <%- partial('_partials/left-panel.ejs') %>
      <%- partial('_partials/right-panel.ejs') %>
    </div>

    <!-- js files -->
    <script src="<%- config.root %>js/jquery.min.js"></script>
    <script src="<%- config.root %>js/main.js"></script>
    <script src="<%- config.root %>js/animationBubble.js"></script>
    <script src="<%- config.root %>js/animationLine.js"></script>
    <script src="<%- config.root %>js/EasePack.min.js"></script>
    <script src="<%- config.root %>js/rAF.js"></script>
    <script src="<%- config.root %>js/TweenLite.min.js"></script>

</body>
</html>
