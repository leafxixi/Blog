<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="chrome=1">


    <title>
      <%= page.title || '' %> 
    </title>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <% if (config.author){ %>
      <meta name="author" content="<%= config.author %>">
    <% } %>
    <% if ( is_home() && config.description){ %>
      <meta name="description" content="<%= config.description %>">
    <% } %>

  
    <%- css('css/leaf.css') %>
    <%- css('css/perfect-scrollbar.min.css') %>

</head>
<body>


    <div class="main-panel">
      <%- partial('_partials/mobile-header', {}, {cache: true}) %>
      <%- partial('_partials/left-panel.ejs', {}, {cache: true}) %>
      <%- partial('_partials/right-panel.ejs', {}, {cache: true}) %>
    </div>

    <!-- js files -->
    <script src="<%- config.root %>js/jquery.min.js"></script>
    <script src="<%- config.root %>js/main.js"></script>
    <script src="<%- config.root %>js/animationBubble.js"></script>
    <script src="<%- config.root %>js/animationLine.js"></script>
    <script src="<%- config.root %>js/EasePack.min.js"></script>
    <script src="<%- config.root %>js/rAF.js"></script>
    <script src="<%- config.root %>js/TweenLite.min.js"></script>
    <script src="<%- config.root %>js/mousewheel.min.js"></script>
    <script src="<%- config.root %>js/perfect-scrollbar.min.js"></script>

</body>
</html>
